<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>CADisableMinimumFrameDurationOnPhone</key>
    <true/>
    <key>CFBundleDevelopmentRegion</key>
    <string>$(DEVELOPMENT_LANGUAGE)</string>
    <key>CFBundleDisplayName</key>
    <string>MobileJarvisNative</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundleIdentifier</key>
    <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>$(PRODUCT_NAME)</string>
    <key>CFBundlePackageType</key>
    <string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0.0</string>
    <key>CFBundleSignature</key>
    <string>????</string>
    <key>CFBundleURLTypes</key>
    <array>
      <!-- Existing schemes -->
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>com.anonymous.MobileJarvisNative</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>exp+mobilejarvisnative</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLName</key>
        <string>mobilejarvisnative-oauth</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>mobilejarvisnative</string>
        </array>
      </dict>
      
      <!-- Google OAuth (existing - will work with your current client ID) -->
      <dict>
        <key>CFBundleURLName</key>
        <string>google-oauth</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>com.googleusercontent.apps.66333577628-jr6ag67m9spk0f96l61moim1jjaus4n7</string>
        </array>
      </dict>
      
      <!-- Microsoft OAuth -->
      <dict>
        <key>CFBundleURLName</key>
        <string>microsoft-oauth</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>msauth.com.anonymous.MobileJarvisNative</string>
        </array>
      </dict>
      
      <!-- Other OAuth Services -->
      <dict>
        <key>CFBundleURLName</key>
        <string>slack-oauth</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>slack</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLName</key>
        <string>notion-oauth</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>notion</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLName</key>
        <string>dropbox-oauth</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>db-YOUR_DROPBOX_APP_KEY</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLName</key>
        <string>todoist-oauth</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>todoist</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLName</key>
        <string>trello-oauth</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>trello</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLName</key>
        <string>zoom-oauth</string>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>zoom</string>
        </array>
      </dict>
    </array>
    <key>CFBundleVersion</key>
    <string>1</string>
    <key>LSRequiresIPhoneOS</key>
    <true/>
    <key>NSAppTransportSecurity</key>
    <dict>
      <key>NSAllowsArbitraryLoads</key>
      <false/>
      <key>NSAllowsLocalNetworking</key>
      <true/>
    </dict>
    <key>UILaunchStoryboardName</key>
    <string>SplashScreen</string>
    <key>UIRequiredDeviceCapabilities</key>
    <array>
      <string>armv7</string>
    </array>
    <key>UIRequiresFullScreen</key>
    <false/>
    <key>UIStatusBarStyle</key>
    <string>UIStatusBarStyleDefault</string>
    <key>UISupportedInterfaceOrientations</key>
    <array>
      <string>UIInterfaceOrientationPortrait</string>
      <string>UIInterfaceOrientationPortraitUpsideDown</string>
    </array>
    <key>UISupportedInterfaceOrientations~ipad</key>
    <array>
      <string>UIInterfaceOrientationPortrait</string>
      <string>UIInterfaceOrientationPortraitUpsideDown</string>
      <string>UIInterfaceOrientationLandscapeLeft</string>
      <string>UIInterfaceOrientationLandscapeRight</string>
    </array>
    <key>UIUserInterfaceStyle</key>
    <string>Light</string>
    <key>UIViewControllerBasedStatusBarAppearance</key>
    <false/>
    
    <!-- LSApplicationQueriesSchemes for OAuth services -->
    <key>LSApplicationQueriesSchemes</key>
    <array>
      <string>googlegmail</string>
      <string>googlecalendar</string>
      <string>slack</string>
      <string>notion</string>
      <string>dropbox</string>
      <string>dropbox-1</string>
      <string>todoist</string>
      <string>trello</string>
      <string>zoomus</string>
      <string>microsoft-outlook</string>
      <string>microsoft-teams</string>
      <string>microsoft-edge</string>
    </array>
  </dict>
</plist>